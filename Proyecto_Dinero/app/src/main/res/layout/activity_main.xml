<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/allef_vinicius_fjtqyzmoh18_unsplash"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-16dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/allef_vinicius_fjtqyzmoh18_unsplash"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-16dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/allef_vinicius_fjtqyzmoh18_unsplash"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-16dp" />

    <Button
        android:id="@+id/btMes"
        android:layout_width="147dp"
        android:layout_height="84dp"
        android:text="Mes"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="211dp"
        tools:layout_editor_absoluteY="585dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="97dp"
        android:layout_height="75dp"
        app:srcCompat="@android:drawable/ic_menu_month"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="585dp" />

    <EditText
        android:id="@+id/TextSalario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="360dp" />

    <TextView
        android:id="@+id/textSal"
        android:layout_width="251dp"
        android:layout_height="66dp"
        android:text="Ingreso Salarial"
        android:textColor="@color/white"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="95dp"
        tools:layout_editor_absoluteY="247dp" />

    <TextView
        android:id="@+id/texlogo"
        android:layout_width="210dp"
        android:layout_height="144dp"
        android:text="CDR"
        android:textColor="#25FD2E"
        android:textSize="96sp"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="77dp" />

    <Button
        android:id="@+id/btMesSigiente"
        android:layout_width="133dp"
        android:layout_height="64dp"
        android:text="Vamos"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="139dp"
        tools:layout_editor_absoluteY="434dp" />


</androidx.constraintlayout.widget.ConstraintLayout>